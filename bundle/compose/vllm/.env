VLLM_IMAGE=forstar/vllm-openai:offline
HOST_DATA_ROOT=/home/ubuntu/data/forstar
HOST_LOG_ROOT=/var/log/forstar

# Model paths inside container (mounted from MODEL_ROOT)
MODEL_ROOT=/opt/models
MODEL_PATH=/models/Qwen3-XXB
MODEL_NAME=Qwen3-XXB

# Memory / performance tuning
GPU_MEMORY_UTILIZATION=0.90
MAX_MODEL_LEN=8192
SWAP_SPACE_GB=16
CPU_OFFLOAD_GB=8

# Host port
VLLM_PORT=8000
