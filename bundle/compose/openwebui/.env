# Image tags (stable offline tags)
OPENWEBUI_IMAGE=forstar/openwebui:offline
RAGFLOW_PROXY_IMAGE=forstar/ragflow-openai-proxy:offline
OPENWEBUI_BASE_PATH=/ai

# Host path layout (absolute paths on server)
HOST_DATA_ROOT=/home/ubuntu/data/forstar
HOST_LOG_ROOT=/var/log/forstar

# OpenWebUI -> RAGFlow proxy
OPENAI_API_KEY=123..

# RAGFlow proxy settings
RAGFLOW_API_KEY=123..
RAGFLOW_MODE=chat
RAGFLOW_CHAT_ID=CHANGE_ME_CHAT_ID
RAGFLOW_AGENT_ID=
MODEL_ID=ragflow-kb
MODEL_NAME=RAGFlow-KB
